{
  "name": "appveyor-desktop",
  "description": "Desktop Automation",
  "main": "build/bundle.js",
  "module": "src/index.js",
  "version": "0.0.1",
  "scripts": {
    "pretest:spectron": "cross-env TEST_SUITE='Full' mocha tests/spectron/conf/configure.js -f Configure",
    "test:spectron:full": "cross-env TEST_SUITE='Full' HTML_REPORT_PATH='test-reports/spectron' mocha --recursive tests/spectron/tests -f '|' --retries 1 --reporter mochawesome --reporter-options reportDir=test-reports/spectron > tee test-reports/spectron/report.txt"
  },
  "keywords": [
    "desktop"
  ],
  "repository": "jiteshsojitra/appveyor-desktop",
  "engines": {
    "node": ">=11.0.0"
  },
  "contributors": [
    "Jitesh Sojitra <jiteshsojitra@gmail.com>"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "lint-staged": {
    "linters": {
      "*.{js,graphql}": [
        "eslint"
      ]
    },
    "ignore": [
      "tests/spectron/**"
    ]
  },
  "devDependencies": {
    "audit-ci": "^1.2.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chalk": "^2.0.1",
    "compression": "^1.7.3",
    "cross-env": "^5.1.6",
    "cross-var": "^1.1.0",
    "download-file-sync": "^1.0.4",
    "electron": "^3.1.8",
    "electron-builder": "^20.39.0",
    "file-matcher": "^1.1.0",
    "husky": "^1.3.1",
    "html-pdf": "^2.2.0",
    "log4js": "^3.0.5",
    "minimist": "^1.2.0",
    "mocha": "^5.0.0",
    "mocha-junit-reporter": "^1.17.0",
    "mochawesome": "^3.1.1",
    "npm-run-all": "^4.1.5",
    "prettier-eslint": "^8.8.0",
    "request": "^2.87.0",
    "request-promise-native": "^1.0.5",
    "sinon": "^7.2.3",
    "sinon-chai": "^3.3.0",
    "spectron": "^5.0.0",
    "superstatic": "^6.0.4"
  },
  "dependencies": {
    "choose-files": "^1.0.1",
    "classnames": "^2.2.5",
    "core-js": "^2.5.7",
    "dataloader": "^1.3.0",
    "escape-string-regexp": "1.0.5",
    "shelljs": "^0.8.3"
  }
}
