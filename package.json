{
  "name": "appveyor-desktop",
  "description": "Desktop Automation",
  "main": "build/bundle.js",
  "module": "src/index.js",
  "version": "0.0.1",
  "scripts": {
    "pretest:spectron": "cross-env TEST_SUITE='Full' mocha tests/spectron/conf/configure.js",
    "test:spectron:full": "cross-env TEST_SUITE='Full' HTML_REPORT_PATH='test-reports/spectron' mocha --recursive tests/spectron/tests -f '|' --retries 1 --reporter mochawesome --reporter-options reportDir=test-reports/spectron | tee test-reports/spectron/report.txt"
  },
  "keywords": [
    "desktop"
  ],
  "repository": "jiteshsojitra/appveyor-desktop",
  "engines": {
    "node": ">=11.0.0"
  },
  "contributors": [
    "Jitesh Sojitra <jiteshsojitra@gmail.com>"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,graphql}": [
        "eslint"
      ]
    },
    "ignore": [
      "tests/spectron/**"
    ]
  },
  "build": {
    "appId": "appveyor-desktop",
    "protocols": {
      "name": "appveyor-desktop",
      "schemes": [
        "mailto"
      ]
    }
  },
  "devDependencies": {
    "audit-ci": "^1.2.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chalk": "^2.0.1",
    "compression": "^1.7.3",
    "cross-env": "^5.1.6",
    "cross-var": "^1.1.0",
    "download-file-sync": "^1.0.4",
    "electron": "^3.1.8",
    "electron-builder": "^20.39.0",
    "file-matcher": "^1.1.0",
    "husky": "^1.3.1",
    "html-pdf": "^2.2.0",
    "lint-staged": "^8.1.5",
    "log4js": "^3.0.5",
    "minimist": "^1.2.0",
    "mocha": "^5.0.0",
    "mocha-junit-reporter": "^1.17.0",
    "mochawesome": "^3.1.1",
    "npm-run-all": "^4.1.5",
    "offline-plugin": "^5.0.6",
    "pdfjs-dist": "^2.0.279",
    "pem": "^1.13.2",
    "prettier-eslint": "^8.8.0",
    "proxy-middleware": "^0.15.0",
    "react-dev-utils": "^5.0.0",
    "recursive-install": "^1.4.0",
    "request": "^2.87.0",
    "request-promise-native": "^1.0.5",
    "sinon": "^7.2.3",
    "sinon-chai": "^3.3.0",
    "spectron": "^5.0.0",
    "superstatic": "^6.0.4",
    "svg-inline-loader": "^0.8.0",
    "vhtml": "^2.1.0",
    "xml-js": "^1.6.11"
  },
  "dependencies": {
    "choose-files": "^1.0.1",
    "classnames": "^2.2.5",
    "clipboard-polyfill": "^2.4.1",
    "core-js": "^2.5.7",
    "dataloader": "^1.3.0",
    "date-fns": "^1.28.5",
    "decko": "^1.2.0",
    "detect-browser": "^4.1.0",
    "dompurify": "^1.0.7",
    "electron-json-storage": "^4.1.2",
    "emojione": "2.2.7",
    "escape-string-regexp": "1.0.5",
    "favico.js": "^0.3.10",
    "focusable": "^2.1.0",
    "font-awesome": "^4.7.0",
    "idb-keyval": "^3.1.0",
    "is-valid-domain": "0.0.5",
    "jszip": "^3.1.5",
    "linkref": "^1.0.1",
    "lunr": "^2.3.4",
    "mime-types": "^2.1.18",
    "mitt": "^1.1.3",
    "moment": "^2.20.1",
    "moment-timezone": "github:aortbals/moment-timezone#fix-module-default-resolution",
    "normalize.css": "^7.0.0",
    "omit-deep": "^0.3.0",
    "preconf": "^1.0.0",
    "pretty-bytes": "^4.0.2",
    "query-string": "^5.0.0",
    "regenerator-runtime": "^0.12.1",
    "scroll-into-view-if-needed": "^1.4.0",
    "shelljs": "^0.8.3",
    "tiny-js-md5": "^1.2.0",
    "wiretie": "^1.0.3"
  }
}
